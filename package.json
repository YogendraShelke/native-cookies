{
  "name": "native-cookies",
  "version": "3.1.1",
  "description": "Cookie manager for React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "native-cookies.podspec",
    "!example",
    "!**/build",
    "!android/.gradle",
    "!android/.idea",
    "!android/.settings"
  ],
  "scripts": {
    "prepare": "bob build",
    "pods": "cd example && pod-install --quiet"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/YogendraShelke/native-cookies",
  "author": "Yogendra Shelke <yogendra.s@somaiya.edu> (https://github.com/YogendraShelke)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YogendraShelke/native-cookies/issues"
  },
  "homepage": "https://github.com/YogendraShelke/native-cookies#readme",
  "devDependencies": {
    "@react-native-community/bob": "^0.15.1",
    "pod-install": "^0.1.0",
    "react": "16.11.0",
    "react-native": "0.62.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  },
  "dependencies": {
    "cookie": "^0.3.1"
  }
}
